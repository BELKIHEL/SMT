<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>IntegrationTemplate</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>RequestServelet</servlet-name>
    <servlet-class>com.SMT.RequestServelet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>RequestServelet</servlet-name>
    <url-pattern>/Request</url-pattern>
</servlet-mapping>
  <servlet>
    <servlet-name>SearchServelet</servlet-name>
    <servlet-class>com.SMT.SearchServelet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>SearchServelet</servlet-name>
    <url-pattern>/Search</url-pattern>
</servlet-mapping>
  <servlet>
    <servlet-name>ConséquenceServelet</servlet-name>
    <servlet-class>com.SMT.ConséquenceServelet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>ConséquenceServelet</servlet-name>
    <url-pattern>/Consequence</url-pattern>
</servlet-mapping>

 
</web-app>